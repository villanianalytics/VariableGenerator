<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity VariableName.
    -->
    <changeSet id="20201206134016-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="listud_id"
                                 baseTableName="variable_name"
                                 constraintName="fk_variable_name_listud_id"
                                 referencedColumnNames="id"
                                 referencedTableName="list_ud"/>

        <addForeignKeyConstraint baseColumnNames="cube_id"
                                 baseTableName="variable_name"
                                 constraintName="fk_variable_name_cube_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cubes"/>

    </changeSet>
</databaseChangeLog>
